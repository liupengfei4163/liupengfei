<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cms.mapper.common.GeneralMstMapper">

	<select id="select"
		parameterType="com.cms.entity.common.GeneralMstBean"
		resultType="com.cms.entity.common.GeneralMstBean">
		SELECT
		generalId
		, generalName
		FROM
		cms_general_mst
		<where>
			<if test="generalKey != null">
				generalKey = #{generalKey}
			</if>
		</where>
		order by generalId
	</select>
</mapper>